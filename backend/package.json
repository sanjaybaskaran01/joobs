{
  "name": "firebase-gmail-server",
  "version": "1.0.0",
  "description": "Node.js TypeScript Firebase server with Google OAuth and Gmail integration",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "start": "node dist/index.js",
    "dev": "ts-node-dev --respawn --transpile-only src/index.ts",
    "clean": "rimraf dist",
    "test:setup": "node scripts/test-setup.js"
  },
  "keywords": ["nodejs", "typescript", "firebase", "oauth", "gmail"],
  "author": "",
  "license": "MIT",
  "dependencies": {
